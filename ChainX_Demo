    <script>
        // Global variables
        let isLoggedIn = false;
        let liveTrackingActive = false;
        let obdDataInterval;

        // IPCC Emission Factors (kg CO‚ÇÇ per liter of fuel)
        const emissionFactors = {
            diesel: 2.68,
            gasoline: 2.31,
            cng: 1.94,
            electric: 0.0,
            hybrid: 1.85
        };

        // Vehicle type fuel consumption factors (L/100km base)
        const vehicleConsumption = {
            light: 8.5,
            medium: 12.0,
            heavy: 18.5,
            articulated: 25.0
        };

        // Show demo function
        function showDemo() {
            document.getElementById('landingPage').classList.add('hidden');
            document.getElementById('loginModal').classList.remove('hidden');
        }

        // Hide demo modal
        function hideDemoModal() {
            document.getElementById('loginModal').classList.add('hidden');
            document.getElementById('landingPage').classList.remove('hidden');
        }

        // Scroll to features
        function scrollToFeatures() {
            document.getElementById('featuresSection').scrollIntoView({ behavior: 'smooth' });
        }

        // Login function
        function login(event) {
            event.preventDefault();
            const companyId = document.getElementById('companyId').value.trim();
            const password = document.getElementById('password').value.trim();
            
            if (companyId && password) {
                isLoggedIn = true;
                document.getElementById('loginModal').classList.add('hidden');
                document.getElementById('mainApp').classList.remove('hidden');
                document.getElementById('companyName').textContent = companyId;
                
                setTimeout(() => {
                    populateRecentCalculations();
                    startDashboardUpdates();
                }, 100);
            } else {
                alert('Please enter both Company ID and Password to access ChainX.');
            }
        }

        // Logout function
        function logout() {
            isLoggedIn = false;
            document.getElementById('mainApp').classList.add('hidden');
            document.getElementById('landingPage').classList.remove('hidden');
            stopLiveTracking();
            if (obdDataInterval) {
                clearInterval(obdDataInterval);
            }
        }

        // Show section
        function showSection(sectionName) {
            document.querySelectorAll('.section').forEach(section => {
                section.classList.add('hidden');
            });
            document.getElementById(sectionName).classList.remove('hidden');
            
            document.querySelectorAll('.sidebar-btn').forEach(btn => {
                btn.classList.remove('sidebar-active');
            });
            document.querySelector(`[data-section="${sectionName}"]`).classList.add('sidebar-active');
        }

        // Calculate emissions using IPCC methodology
        function calculateEmissions(event) {
            event.preventDefault();
            
            const origin = document.getElementById('origin').value;
            const destination = document.getElementById('destination').value;
            const vehicleType = document.getElementById('vehicleType').value;
            const fuelType = document.getElementById('fuelType').value;
            const loadWeight = parseFloat(document.getElementById('loadWeight').value) || 0;
            const vehicleAge = parseFloat(document.getElementById('vehicleAge').value) || 0;
            const engineSize = parseFloat(document.getElementById('engineSize').value) || 2.0;
            const loadFactor = parseFloat(document.getElementById('loadFactor').value) || 50;

            // Simulate route calculation
            const distance = Math.floor(Math.random() * 300) + 200; // km
            
            // IPCC calculation
            let baseFuelConsumption = vehicleConsumption[vehicleType];
            
            // Adjust for load factor
            const loadAdjustment = 1 + (loadFactor / 100) * 0.3;
            baseFuelConsumption *= loadAdjustment;
            
            // Adjust for vehicle age
            const ageAdjustment = 1 + (vehicleAge * 0.02);
            baseFuelConsumption *= ageAdjustment;
            
            // Adjust for engine size
            const engineAdjustment = engineSize / 2.0;
            baseFuelConsumption *= engineAdjustment;
            
            // Calculate total fuel consumption
            const totalFuel = (baseFuelConsumption / 100) * distance;
            
            // Calculate CO‚ÇÇ emissions using IPCC factors
            const totalCO2 = totalFuel * emissionFactors[fuelType];
            const co2PerKm = (totalCO2 / distance) * 1000; // g/km
            
            // Display results
            document.getElementById('totalCO2').textContent = totalCO2.toFixed(1);
            document.getElementById('co2PerKm').textContent = co2PerKm.toFixed(0);
            document.getElementById('fuelConsumption').textContent = totalFuel.toFixed(1);
            document.getElementById('routeDistance').textContent = distance.toFixed(0);
            
            // Categorize emissions
            const categoryElement = document.getElementById('emissionCategory');
            const categoryText = document.getElementById('categoryText');
            const categoryDescription = document.getElementById('categoryDescription');
            
            if (co2PerKm < 120) {
                categoryElement.className = 'mt-6 p-4 rounded-lg co2-low';
                categoryText.textContent = 'Low Emissions ‚úÖ';
                categoryDescription.textContent = 'Excellent performance! This route has low environmental impact.';
            } else if (co2PerKm < 180) {
                categoryElement.className = 'mt-6 p-4 rounded-lg co2-medium';
                categoryText.textContent = 'Moderate Emissions ‚ö†Ô∏è';
                categoryDescription.textContent = 'Good performance with room for optimization.';
            } else {
                categoryElement.className = 'mt-6 p-4 rounded-lg co2-high';
                categoryText.textContent = 'High Emissions ‚ùå';
                categoryDescription.textContent = 'Consider route optimization or vehicle efficiency improvements.';
            }
        }

        // Start live tracking
        function startLiveTracking() {
            liveTrackingActive = true;
            updateVehiclePositions();
            
            setInterval(() => {
                if (liveTrackingActive) {
                    updateVehiclePositions();
                }
            }, 5000);
        }

        // Stop live tracking
        function stopLiveTracking() {
            liveTrackingActive = false;
        }

        // Update vehicle positions
        function updateVehiclePositions() {
            const now = new Date().toLocaleTimeString();
            document.getElementById('trackingLastUpdate').textContent = now;
        }

        // Select vehicle
        function selectVehicle() {
            const selected = document.getElementById('vehicleSelect').value;
            console.log('Selected vehicle:', selected);
        }

        // Simulate OBD-II data
        function simulateOBDData() {
            if (obdDataInterval) {
                clearInterval(obdDataInterval);
            }
            
            obdDataInterval = setInterval(() => {
                // Simulate real-time OBD data
                const rpm = Math.floor(Math.random() * 1000) + 1500;
                const speed = Math.floor(Math.random() * 40) + 50;
                const fuelRate = (Math.random() * 4 + 6).toFixed(1);
                const engineLoad = Math.floor(Math.random() * 30) + 30;
                
                document.getElementById('engineRPM').textContent = rpm.toLocaleString();
                document.getElementById('vehicleSpeed').textContent = speed;
                document.getElementById('fuelRate').textContent = fuelRate;
                document.getElementById('engineLoad').textContent = engineLoad;
                
                // Update sensor data
                const o2Voltage = (Math.random() * 0.4 + 0.6).toFixed(2);
                const mafRate = (Math.random() * 5 + 10).toFixed(1);
                const coolantTemp = Math.floor(Math.random() * 20) + 80;
                
                document.getElementById('o2Sensor').textContent = o2Voltage + 'V';
                document.getElementById('mafSensor').textContent = mafRate + ' g/s';
                document.getElementById('coolantTemp').textContent = coolantTemp + '¬∞C';
                
                // Update progress bars
                document.getElementById('o2SensorBar').style.width = (parseFloat(o2Voltage) / 1.2 * 100) + '%';
                document.getElementById('mafSensorBar').style.width = (parseFloat(mafRate) / 20 * 100) + '%';
                document.getElementById('coolantTempBar').style.width = (coolantTemp / 120 * 100) + '%';
                
                // Calculate real-time CO‚ÇÇ
                const realTimeCO2 = Math.floor(fuelRate * 2.68 / speed * 100000);
                document.getElementById('realTimeCO2').textContent = realTimeCO2;
                
                // Update trip averages
                const avgCO2 = Math.floor(realTimeCO2 * (0.9 + Math.random() * 0.2));
                const totalTripCO2 = (avgCO2 * speed * 0.1 / 1000).toFixed(1);
                const efficiency = (parseFloat(fuelRate) / speed * 100).toFixed(1);
                
                document.getElementById('avgCO2').textContent = avgCO2;
                document.getElementById('totalTripCO2').textContent = totalTripCO2;
                document.getElementById('fuelEfficiency').textContent = efficiency;
                
            }, 2000);
        }

        // Generate report
        function generateReport() {
            const reportDate = new Date().toLocaleDateString('en-US', { 
                year: 'numeric', 
                month: 'long', 
                day: 'numeric' 
            });
            document.getElementById('reportDate').textContent = reportDate;
            
            document.getElementById('sampleReport').scrollIntoView({ behavior: 'smooth' });
        }

        // Optimization functions
        function optimizeRoutes() {
            alert('üó∫Ô∏è Route optimization initiated! AI algorithms are analyzing traffic patterns and emission data to suggest optimal routes.');
        }

        function optimizeLoads() {
            alert('üì¶ Load optimization started! Analyzing cargo distribution patterns to maximize vehicle utilization.');
        }

        function analyzeDriving() {
            alert('üöó Driver behavior analysis in progress! OBD-II data is being processed to identify eco-driving opportunities.');
        }

        function predictMaintenance() {
            alert('üîß Predictive maintenance analysis running! Sensor data indicates TRK-002 needs attention within 2 weeks.');
        }

        function optimizeFleet() {
            alert('üöõ Fleet composition analysis initiated! Evaluating vehicle mix and fuel alternatives for maximum efficiency.');
        }

        function calculateOffsets() {
            alert('üå± Carbon offset calculation complete! Recommend 2.8 tons of verified carbon credits for full neutrality.');
        }

        // Populate recent calculations
        function populateRecentCalculations() {
            const calculations = [
                { route: 'New York ‚Üí Boston', co2: '45.2 kg', efficiency: 'Good', time: '2 hours ago' },
                { route: 'Chicago ‚Üí Detroit', co2: '67.8 kg', efficiency: 'Moderate', time: '4 hours ago' },
                { route: 'LA ‚Üí San Francisco', co2: '89.1 kg', efficiency: 'Poor', time: '6 hours ago' }
            ];
            
            const container = document.getElementById('recentCalculations');
            container.innerHTML = calculations.map(calc => `
                <div class="flex items-center justify-between p-4 bg-gray-50 rounded-lg">
                    <div>
                        <p class="font-medium text-gray-800">${calc.route}</p>
                        <p class="text-sm text-gray-600">${calc.time}</p>
                    </div>
                    <div class="text-right">
                        <p class="font-medium text-gray-800">${calc.co2}</p>
                        <p class="text-sm ${calc.efficiency === 'Good' ? 'text-green-600' : calc.efficiency === 'Moderate' ? 'text-yellow-600' : 'text-red-600'}">${calc.efficiency}</p>
                    </div>
                </div>
            `).join('');
        }

        // Start dashboard updates
        function startDashboardUpdates() {
            setInterval(() => {
                const activeCount = Math.floor(Math.random() * 3) + 10;
                document.getElementById('activeVehicles').textContent = activeCount;
                
                const currentEmissions = parseFloat(document.getElementById('totalEmissions').textContent.replace(' kg', '').replace(',', ''));
                const newEmissions = currentEmissions + (Math.random() - 0.5) * 10;
                document.getElementById('totalEmissions').textContent = newEmissions.toLocaleString() + ' kg';
                
                const now = new Date().toLocaleTimeString();
                document.getElementById('lastUpdate').textContent = now;
                
            }, 10000);
        }

        // Add quick trip function
        function addQuickTrip(event) {
            event.preventDefault();
            
            const vehicleId = document.getElementById('quickVehicleId').value;
            const distance = parseFloat(document.getElementById('quickDistance').value);
            const fuel = parseFloat(document.getElementById('quickFuel').value);
            const load = parseFloat(document.getElementById('quickLoad').value);
            
            // Calculate CO‚ÇÇ emissions
            const co2 = fuel * 2.68; // kg CO‚ÇÇ
            const efficiency = (fuel / distance * 100).toFixed(1); // L/100km
            
            // Update dashboard stats
            const currentEmissions = parseFloat(document.getElementById('totalEmissions').textContent.replace(' kg', '').replace(',', ''));
            const newTotal = currentEmissions + co2;
            document.getElementById('totalEmissions').textContent = newTotal.toLocaleString() + ' kg';
            
            // Show success message
            alert(`‚úÖ Trip added successfully!\n\nVehicle: ${vehicleId}\nDistance: ${distance} km\nCO‚ÇÇ Emissions: ${co2.toFixed(1)} kg\nFuel Efficiency: ${efficiency} L/100km`);
            
            // Reset form with new values
            document.getElementById('quickVehicleId').value = 'TRK-' + String(Math.floor(Math.random() * 900) + 100).padStart(3, '0');
            document.getElementById('quickDistance').value = Math.floor(Math.random() * 200) + 50;
            document.getElementById('quickFuel').value = (Math.random() * 10 + 5).toFixed(1);
            document.getElementById('quickLoad').value = Math.floor(Math.random() * 2000) + 1000;
        }

        // Add new vehicle function
        function addNewVehicle(event) {
            event.preventDefault();
            
            const vehicleId = document.getElementById('newVehicleId').value;
            const vehicleType = document.getElementById('newVehicleType').value;
            const fuelType = document.getElementById('newVehicleFuel').value;
            
            // Add to vehicle select dropdown
            const vehicleSelect = document.getElementById('vehicleSelect');
            const option = document.createElement('option');
            option.value = vehicleId;
            option.textContent = `${vehicleId} - ${vehicleType.charAt(0).toUpperCase() + vehicleType.slice(1)} Truck (New)`;
            vehicleSelect.appendChild(option);
            
            // Update active vehicles count
            const currentActive = parseInt(document.getElementById('activeVehicles').textContent);
            document.getElementById('activeVehicles').textContent = currentActive + 1;
            
            // Show success message
            alert(`üöõ Vehicle ${vehicleId} added successfully!\n\nType: ${vehicleType.charAt(0).toUpperCase() + vehicleType.slice(1)} Truck\nFuel: ${fuelType.charAt(0).toUpperCase() + fuelType.slice(1)}\n\nVehicle is now available for tracking and data entry.`);
            
            // Reset form with new ID
            const nextId = parseInt(vehicleId.split('-')[1]) + 1;
            document.getElementById('newVehicleId').value = 'TRK-' + String(nextId).padStart(3, '0');
        }

        // Add manual OBD data function
        function addManualOBDData(event) {
            event.preventDefault();
            
            const vehicleId = document.getElementById('obdVehicleId').value;
            const rpm = document.getElementById('manualRPM').value;
            const speed = document.getElementById('manualSpeed').value;
            const fuelRate = document.getElementById('manualFuelRate').value;
            const engineLoad = document.getElementById('manualEngineLoad').value;
            
            // Update the live display with manual data
            document.getElementById('engineRPM').textContent = parseInt(rpm).toLocaleString();
            document.getElementById('vehicleSpeed').textContent = speed;
            document.getElementById('fuelRate').textContent = fuelRate;
            document.getElementById('engineLoad').textContent = engineLoad;
            
            // Calculate real-time CO‚ÇÇ
            const realTimeCO2 = Math.floor(parseFloat(fuelRate) * 2.68 / parseFloat(speed) * 100000);
            document.getElementById('realTimeCO2').textContent = realTimeCO2;
            
            // Show success message
            alert(`üìä OBD-II data recorded for ${vehicleId}!\n\nRPM: ${rpm}\nSpeed: ${speed} km/h\nFuel Rate: ${fuelRate} L/h\nEngine Load: ${engineLoad}%\nCalculated CO‚ÇÇ: ${realTimeCO2} g/km`);
            
            // Generate new sample values
            document.getElementById('manualRPM').value = Math.floor(Math.random() * 1000) + 1500;
            document.getElementById('manualSpeed').value = Math.floor(Math.random() * 40) + 50;
            document.getElementById('manualFuelRate').value = (Math.random() * 4 + 6).toFixed(1);
            document.getElementById('manualEngineLoad').value = Math.floor(Math.random() * 30) + 30;
        }

        // Initialize on page load
        document.addEventListener('DOMContentLoaded', function() {
            // Show landing page by default
            document.getElementById('landingPage').classList.remove('hidden');
        });
    </script>
